{
  "rewrites": [
    { "source": "/sw.js", "destination": "/sw.js" },
    { "source": "/workbox-:hash.js", "destination": "/workbox-:hash.js" },
    {
      "source": "/firebase-messaging-sw.js",
      "destination": "/firebase-messaging-sw.js"
    },
    { "source": "/(.*)", "destination": "/" }
  ],
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
